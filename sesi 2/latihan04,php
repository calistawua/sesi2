<?php
    $nVar = "dua";
    switch($nVar){
        case "satu";
            echo "nVar bernilai Satu";
            break;
        case "dua";
            echo "nVar bernilai Dua";
            break;
        case "tiga";
            echo "nVar bernilai Tiga";
            break;
        default:
            echo "nVar bernilai acak";
            break;
    }